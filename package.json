{
  "name": "carbon-fiber",
  "version": "0.0.1",
  "description": "Carbon Fiber components",
  "license": "MIT",
  "author": "sassan",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anistodev/carbon-fiber.git"
  },
  "homepage": "https://github.com/Anistodev/carbon-fiber#readme",
  "bugs": {
    "url": "https://github.com/Anistodev/carbon-fiber/issues"
  },
  "files": [
    "dist",
    "tailwind.config.js"
  ],
